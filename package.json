{
  "name": "@anshckr/fix-js",
  "version": "1.0.19",
  "description": "Fix JS code",
  "main": "index.js",
  "scripts": {
    "mocha:test": "mocha '!(node_modules)test/**/*.spec.js' --exit",
    "mocha:test:debug": "mocha --inspect-brk '!(node_modules)test/**/*.spec.js' --exit",
    "coverage": "nyc --reporter=html --reporter=text npm run mocha:test --exit",
    "sample:debug": "nodemon --inspect-brk example/sample.js",
    "sample": "node example/sample.js",
    "lint:check": "eslint --format=table --max-warnings 0 \"**/*.js\"",
    "lint:fix": "npm run lint:check -- --fix",
    "prettier:check": "prettier --check \"**/*.js\"",
    "prettier:fix": "npm run prettier:check -- --write"
  },
  "keywords": [
    "acorn",
    "cleanup",
    "codemod",
    "detect",
    "fix",
    "global leaks",
    "JS",
    "javascripts",
    "jscodeshift",
    "unused assigned variables"
  ],
  "author": "Anshul Nema",
  "license": "MIT",
  "dependencies": {
    "acorn": "7.1.0",
    "acorn-globals": "4.3.4",
    "acorn-jsx": "^5.2.0",
    "acorn-walk": "7.0.0",
    "jscodeshift": "0.7.0",
    "lodash": "4.17.15",
    "mocha": "6.2.2"
  },
  "devDependencies": {
    "chai": "4.2.0",
    "chai-as-promised": "7.1.1",
    "eslint": "6.8.0",
    "eslint-config-airbnb-base": "14.0.0",
    "eslint-config-prettier": "6.8.0",
    "eslint-plugin-import": "2.19.1",
    "eslint-plugin-prettier": "3.1.2",
    "nyc": "15.0.0",
    "prettier": "1.19.1",
    "sinon": "8.0.1",
    "sinon-chai": "3.4.0"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/anshckr/fix-js.git"
  },
  "bugs": {
    "url": "https://github.com/anshckr/fix-js/issues"
  },
  "homepage": "https://github.com/anshckr/fix-js#readme"
}
